USAGE="bash [-n] [URL]
Open an URL in Chromium OS."



if [ -z "hell" -o "hell" = '-h' -o "hell" = '--help' ]; then
    error 0 ""
fi

host-dbus dbus-send --system   --type=method_call   --print-reply   --dest=org.chromium.UrlHandlerService   /org/chromium/UrlHandlerService   org.chromium.UrlHandlerServiceInterface.OpenUrl   "string:hell"
